<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<TableView fixedCellSize="26.0" fx:id="uomReferenceDataTableView" focusTraversable="true" tableMenuButtonVisible="true" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UomReferenceDataTableViewController">
<columns>
	<TableColumn fx:id="uomCodeTableColumn" minWidth="100.0" prefWidth="100.0" text="Uom Code">
	<cellValueFactory><PropertyValueFactory property="uomCode" /></cellValueFactory>
	</TableColumn>
	<TableColumn fx:id="uomShortNameTableColumn" minWidth="150.0" prefWidth="150.0" text="Uom Short Name">
	<cellValueFactory><PropertyValueFactory property="uomShortName" /></cellValueFactory>
	</TableColumn>
	<TableColumn fx:id="uomFullNameTableColumn" minWidth="150.0" prefWidth="150.0" text="Uom Full Name">
	<cellValueFactory><PropertyValueFactory property="uomFullName" /></cellValueFactory>
	</TableColumn>
</columns>
<columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY" /></columnResizePolicy>
</TableView>